import java.util.*;
public class sq_cu
{
	public static void main(String[] args) 
	{
		float m;
		Scanner sc=new Scanner(System.in);
		System.out.print("Given Number: ");
		m=sc.nextFloat();
      if(m<=0)
      {
      System.out.println("INVALID");
      }
      else
      {
		System.out.print("Square Number: "+(m*m)+"\n");
		System.out.print("Cube Number: "+(m*m*m));
      }
	}
}
